{
  "name": "proyecto-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "check-naming-convention": "node ./naming-checker.mjs",
    "start": "ts-node src/main.ts",
    "start:prod": "tsc && copyfiles -u  1 src/assets/* dist && node dist/main.js",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint .",
    "test:file": "jest --testPathPattern",
    "precommit": "npm run lint",
    "prepare": "husky",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.12",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-naming-convention && npx lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/node": "^20.11.16",
    "csv-parser": "^3.0.0",
    "undici-types": "^6.6.2",
    "xml2js": "^0.6.2",
    "yup": "^1.3.3"
  }
}
